{
  "page": {
    "key": "Platform",
    "color": "#1D0309"
  },
  "datasources": [
    "products",
    "solutions",
    "concepts",
    "latest-footer"
  ],
  "events": [
    "globals"
  ],
  "requiredDatasources": [
    "products"
  ],
  "routes": [
    {
      "path": "/platform/"
    },
    {
      "path": "/platform/:product",
      "fetch": "products"
    },
    {
      "path": "/platform/:page",
      "params": [
        {
          "param": "page",
          "in": ["security", "licensing", "roadmap", "concepts"]
        }
      ]
    },
    {
      "path": "/platform/:page/:concept",
      "params": [
        {
          "param": "concept",
          "fetch": "concepts"
        }
      ]
    }
  ]
} 